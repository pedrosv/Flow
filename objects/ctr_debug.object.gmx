            <string>///Draw debug information (edit me)

draw_set_color(c_red);
draw_set_halign(fa_left);

with(jce_obj_touch_event){
    draw_line(x,y,room_width/2,room_height/2);
    var pdir = point_direction(x,y,room_width/2,room_height/2);
    var pdis = point_distance(x,y,room_width/2,room_height/2);
    draw_text(x+lengthdir_x(pdis/2,pdir),y+lengthdir_y(pdis/2,pdir),string(touch_number));
}

with(par_button){
    draw_text(x,y-96,string(pressed_by) + "#" + string(is_over));
}

with(ctr_screen){
    var str;
    switch(screen_scale_type)
    {
        case 0: str = "FULL - Locked at 100% (Desktop)";break;
        case 1: str = "FIT - Fit game inside the screen (Mobile)";break;
        case 2: str = "FILL - Fill the screen (Mobile)";break;
        case 3: str = "FIX - Fixed ratio, maxed at 100% (Desktop)";break;
        case 4: str = "FLEX - Does not keep aspect ratio";break;
    }
    
    draw_set_color(c_black);
    jce_format_text(fa_left,fa_top,fnt_jchtml5);
    
    if (view_enabled){
        draw_text(32,32,str+
        "#View size locked - " + string(view_size_locked) +
        "#View enabled - " + string(view_enabled) +
        "#Desired rot. - " + string(desired_rotation) +
        "#Actual rot. - " + string(actual_rotation) +
        "#Browser size: "+string(browser_width) + '  x  ' + string(browser_height) +
        "#Window size: "+string(window_get_width()) + '  x  ' + string(window_get_height()) +
        "#View size: "+string(view_wview[0]) + '  x  ' + string(view_hview[0]) +
        "#Port size: "+string(view_wport[0]) + '  x  ' + string(view_hport[0]));
    } else {
        draw_text(32,32,str+
        "#View is disabled - " +
        "#Desired rot. - " + string(desired_rotation) +
        "#Actual rot. - " + string(actual_rotation) +
        "#Browser size: "+string(browser_width) + '  x  ' + string(browser_height) +
        "#Window size: "+string(window_get_width()) + '  x  ' + string(window_get_height()));
    }
}

